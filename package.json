  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.28",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@blockworks-foundation/mango-client": "^3.0.12",
    "@solana/web3.js": "^1.18.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "node-cron": "^2.0.3",
    "pg": "^8.5.1",
